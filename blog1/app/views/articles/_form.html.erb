<%= form_for article do|f| %>
<div class="form-group">
  <p><%= f.label :title,'タイトル'%></p>
  <p><%= f.text_field :title %></p>
</div>
<div class="form-group">
    <p><%= f.label :body,'記事内容'%></p>
    <p><%= f.text_area :body %></p>
</div>
<div class="form-group">
  <%= f.submit %>
</div>
<% end %>
